{
  "name": "project",
  "version": "1.0.0",
  "description": "boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "webpack",
    "build": "SET \"NODE_ENV=production\" && webpack && node server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/41x/project.git"
  },
  "author": "alxnam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/41x/project/issues"
  },
  "homepage": "https://github.com/41x/project#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon-webpack-plugin": "^4.0.3",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "dependencies": {
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^1.5.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb"
    ],
    "rules": {
      "strict": 0,
      "import/no-extraneous-dependencies": "off",
      "css-modules/no-unused-class": "off",
      "react/jsx-filename-extension": "off",
      "comma-dangle": "off",
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "react/jsx-indent": [
        "error",
        4
      ],
      "react/jsx-indent-props": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "css-modules/no-undef-class": "off",
      "arrow-body-style": "off",
      "import/prefer-default-export": "off",
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "react/prefer-stateless-function": "off",
      "react/sort-comp": "off",
      "react/require-default-props": "off",
      "object-curly-newline": "off"
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "NODE_ENV": false
    }
  }
}
